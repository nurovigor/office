{
  "compilerOptions": {
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "src/*": [
        "./src/*"
      ]
    },
    "compilerOptions": {
      "allowJs": true
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ]
}
